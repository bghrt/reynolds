.ui-tabs{
	position: relative;
	padding: .1em;
	zoom: 1;
	margin-top:.5em;
	background-color:#a32638;
	padding:10px; 
	border-radius:10px; 
	box-shadow: 10px 10px 15px #888888;
	
	
}

.ui-tabs .ui-tabs-nav{
	margin: 0;
	padding:  0;
	color: #ffffff;}
.yui-content li a {font-size:18px;}	

.ui-tabs .ui-tabs-nav li{
	list-style: none;
	float: left;
	position: relative;
	top: 0;
	
	border-bottom: 0;
	padding: 0;
	white-space: nowrap;
	border-top-right-radius:.65em; border-top-left-radius:.75em;
	
}


.ui-tabs .ui-tabs-nav li a{
	float: left;
	padding: .5em 1em;
	text-decoration: none;
	color: #ffffff;
	background-color: #8c0000; 
	border-radius:10px; 
	
	margin-right:3px;
	
	
}

.ui-tabs .ui-tabs-nav li a:hover{
	background-color: lightgray;
	color:#a32638;		
}
	
